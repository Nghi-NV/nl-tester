  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "download-binaries": "node -e \"const { execSync } = require('child_process'); const os = require('os'); const platform = os.platform(); if (platform === 'win32') { execSync('powershell -ExecutionPolicy Bypass -File src-tauri/scripts/download_binaries.ps1', { stdio: 'inherit' }); } else { execSync('bash src-tauri/scripts/download_binaries.sh', { stdio: 'inherit' }); }\"",
    "tauri:build": "npm run download-binaries && tauri build"
  },
